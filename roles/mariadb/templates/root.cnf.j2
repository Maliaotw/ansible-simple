[mysqld]
default-character-set=utf8
default-collation=utf8_unicode_ci
character-set-server=utf8
collation-server=utf8_unicode_ci 
 
datadir=/var/lib/mysql
socket=/var/lib/mysql/mysql.sock
 
[client]
default-character-set=utf8
user=root
password={{ mysql_new_password }}

[mysql]
default-character-set=utf8